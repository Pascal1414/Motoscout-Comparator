{
    "manifest_version": 3,
    "name": "Motorcycle Comparator",
    "description": "Motorcycle Comparator",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*.motoscout24.ch/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "assets/chart.js"
            ],
            "matches": [
                "*://*.motoscout24.ch/*"
            ]
        }
    ]
}